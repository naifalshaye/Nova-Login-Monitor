{
    "name": "naif/nova-login-monitor",
    "description": "Monitor Nova Login via Slack notifications, Receive a notification every time someone's login to your system.",
    "keywords": [
        "laravel",
        "nova",
        "monitor",
        "login",
        "auth",
        "slack",
        "notifications"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Naif\\NovaLoginMonitor\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Naif\\NovaLoginMonitor\\CardServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
